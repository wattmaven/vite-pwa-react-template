{
  "name": "@wattmaven/vite-pwa-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install:hooks": "lefthook install",
    "commit": "commit",
    "commitlint": "commitlint",
    "format:check:prettier": "prettier --check .",
    "format:fix:prettier": "prettier --write .",
    "format:check": "run-s format:check:*",
    "format:fix": "run-s format:fix:*",
    "lint:check": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "fix": "run-s *:fix",
    "generate:assets": "pwa-assets-generator",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "clean": "rimraf dev-dist dist .turbo .vercel node_modules",
    "pre-commit-tasks": "turbo format:check lint:check build",
    "ci:smoke-test": "turbo format:check lint:check build",
    "ci:version": "changeset version",
    "ci:publish": "changeset publish"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.12",
    "@tanstack/react-router": "^1.114.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.12"
  },
  "devDependencies": {
    "@changesets/cli": "2.28.1",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/prompt-cli": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@eslint/js": "9.23.0",
    "@tanstack/router-devtools": "1.114.29",
    "@tanstack/router-plugin": "1.114.29",
    "@types/node": "22.13.14",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vite-pwa/assets-generator": "0.2.6",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "lefthook": "1.11.5",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "rimraf": "6.0.1",
    "turbo": "2.4.4",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0",
    "vite": "6.2.3",
    "vite-plugin-pwa": "0.21.2",
    "workbox-core": "7.3.0",
    "workbox-window": "7.3.0"
  },
  "resolutions": {
    "sharp": "0.33.5",
    "sharp-ico": "0.1.5"
  },
  "packageManager": "pnpm@9.15.9"
}
